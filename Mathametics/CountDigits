#include <iostream>
using namespace std ;

int CountDigits( int a [ ], int t  )
{
    int i ;
    for( i = 0 ; i < t ; i++ )
    {
       int count = 0 ;
        while(a[i] != 0)
        {
            a[i] = a[i]/10 ;
            count++ ;
        }
        if(a[i] == 0)
            cout << count << endl ;
    }
    return 0 ;
}  
int main()
{
    int t , i ;
    cin >> t ;
    int a[t] ;
    for( i = 0 ; i < t ; i++ )
    {
        cin >> a[i] ;
    }
    CountDigits( a , t ) ;
return 0 ;
}
